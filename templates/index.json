    "custom_liquid_7q94UJ": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<style>\n  /* Desktop: images side by side */\n  .flush-images {\n    display: flex;\n    flex-wrap: nowrap;\n    gap: 0;\n  }\n\n  .flush-images a {\n    flex: 1 1 50%;\n    width: 50%;\n    display: block;\n    overflow: hidden;\n  }\n\n  .flush-images img {\n    width: 100%;\n    height: auto;\n    display: block;\n    transition: transform 0.4s ease;\n  }\n\n  /* Zoom on hover (desktop only) */\n  @media (min-width: 769px) {\n    .flush-images a:hover img {\n      transform: scale(1.05);\n    }\n  }\n\n  /* Mobile: horizontal scroll */\n  @media (max-width: 768px) {\n    .flush-images {\n      display: flex;\n      overflow-x: auto;\n      scroll-snap-type: x mandatory;\n      -webkit-overflow-scrolling: touch;\n    }\n\n    .flush-images a {\n      flex: 0 0 100%; /* Each slide takes full width */\n      scroll-snap-align: start;\n    }\n\n    /* Hide scrollbar */\n    .flush-images::-webkit-scrollbar {\n      display: none;\n    }\n    .flush-images {\n      -ms-overflow-style: none;\n      scrollbar-width: none;\n    }\n\n    /* Dots below the images */\n    .flush-dots {\n      display: flex;\n      justify-content: center;\n      gap: 8px;\n      margin-top: 8px;\n    }\n\n    .flush-dots span {\n      width: 8px;\n      height: 8px;\n      background: #ccc;\n      border-radius: 50%;\n      display: inline-block;\n      transition: background 0.3s ease;\n    }\n\n    .flush-dots span.active {\n      background: #000;\n    }\n  }\n</style>\n\n<div class=\"flush-wrapper\">\n  <div class=\"flush-images\" id=\"flush-slider\">\n    <a href=\"https://henleysapparel.com/collections/t-shirts\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0818/2412/1171/files/QUICK_LINKS_-_Tees_345b8863-ec93-42a8-9029-1527587e0778.png?v=1755184572\" alt=\"Shop Tees\">\n    </a>\n    <a href=\"https://henleysapparel.com/collections/shorts\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0818/2412/1171/files/QUICK_LINKS_-_Shorts_2952b394-c9dd-4787-9087-552e3f5c61c5.png?v=1755514710\" alt=\"Shop Shorts\">\n    </a>\n  </div>\n\n  <!-- Dots below images (not absolutely positioned) -->\n  <div class=\"flush-dots\" id=\"flush-dots\">\n    <span class=\"active\"></span>\n    <span></span>\n  </div>\n</div>\n\n<script>\n  const slider = document.getElementById('flush-slider');\n  const dots = document.getElementById('flush-dots').children;\n\n  slider.addEventListener('scroll', () => {\n    const index = Math.round(slider.scrollLeft / slider.clientWidth);\n    for (let i = 0; i < dots.length; i++) {\n      dots[i].classList.toggle('active', i === index);\n    }\n  });\n</script>"
      }
    },
